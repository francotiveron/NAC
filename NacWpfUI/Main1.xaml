<ContentControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Nac.Wpf.UI"
             xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" x:Class="Nac.Wpf.UI.Main1"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Label x:Name="label" Content="{Binding Ciccia}" HorizontalAlignment="Left" Height="26" Margin="69,129,0,0" VerticalAlignment="Top" Width="115"/>
        <Label x:Name="label1" Content="Label" HorizontalAlignment="Left" Margin="93,78,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="140,32,0,0" TextWrapping="Wrap" Text="{Binding Ciccia}" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox_Copy" HorizontalAlignment="Left" Height="23" Margin="151,93,0,0" TextWrapping="Wrap" Text="{Binding Ciccia}" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Height="27" Margin="79,188,0,0" VerticalAlignment="Top" Width="74" Click="button_Click_1"/>
    </Grid>
</ContentControl>
